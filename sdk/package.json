{
  "name": "@bhavishyaone/sdk",
  "version": "1.0.0",
  "description": "Lightweight analytics SDK for @bhavishyaone/sdk",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.js --format cjs,esm,iife --global-name bhavishyaone --minify",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "analytics",
    "bhavishyaone",
    "tracking",
    "events"
  ],
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  }
}